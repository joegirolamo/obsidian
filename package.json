{
  "name": "obsidian",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "cd apps/connect && npm run build",
    "dev": "cd apps/connect && npm run dev",
    "dev:port": "cd apps/connect && next dev -p",
    "lint": "cd apps/connect && npm run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "prettier": "^3.0.0"
  },
  "packageManager": "npm@10.2.4",
  "dependencies": {
    "@prisma/client": "^6.5.0",
    "prisma": "^6.5.0"
  }
}
